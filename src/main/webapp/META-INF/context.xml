<?xml version="1.0" encoding="UTF-8"?>
<Context>
    <Resource
        name="jdbc/register"
        auth="Container"
        type="javax.sql.DataSource"
        username="postgres"
        password="123"
        driverClassName="org.postgresql.Driver"
        url="jdbc:postgresql://localhost:5434/postgres"
        validationQuery="select 1"
        maxActive="10"
        maxWait="5000"
        maxIdle="4">
    </Resource>
</Context>
